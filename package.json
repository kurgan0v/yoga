    "@telegram-apps/sdk-react": "^3.1.7",
    "@telegram-apps/telegram-ui": "^2.1.8",
    "@tonconnect/ui-react": "^2.1.0",
    "dotenv": "^16.5.0",
    "eruda": "^3.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.23.0",
